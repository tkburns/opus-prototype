{
  "name": "opus-prototype",
  "version": "0.0.0",
  "description": "prototype compiler for the opus programming language",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json -w",
    "clean": "rm -rf build/*",
    "start:pure": "node ./build/index.js",
    "start": "npm run build && npm run start:pure",
    "test": "jest --config ./test/jest.config.js",
    "test:coverage": "jest --config ./test/jest.config.js --coverage",
    "test:watch": "jest --config ./test/jest.config.js --watch"
  },
  "keywords": [
    "opus",
    "language",
    "compiler",
    "algebraic-effects"
  ],
  "author": "Tate Burns",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/jest": "^26.0.20",
    "@types/minimist": "^1.2.1",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.21",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-watch-typeahead": "^0.6.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "minimist": "^1.2.5",
    "module-alias": "^2.2.2"
  }
}
